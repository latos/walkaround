.panel {
  text-align: left;
  overflow: auto;
  width: 100%;
  height: 100%;
}

.entry {
  border-bottom:1px dotted silver;
  font-family: monospace;
}

.module {
  font-weight:bold;
}

.error {
  color:red;
}

.fatal {
  color:red;
  font-weight:bold;
}

.trace {
  /* Stylename set by Logger but not yet used */
}

.time {
  /* Stylename set by Logger but not yet used */
}

.msg {
  /* Stylename set by Logger but not yet used */
}
